	NAME "CH03_15"

STACK	SEGMENT	STACK
	DD	128	DUP(00000000H)
STACK	ENDS
	
DATA	SEGMENT
BLANK0	DB	02000H	DUP(00H)
ARRAY	DD	12345678H,	90ABCDEFH,	23456789H
DATA	ENDS

CODE	SEGMENT
	ASSUME  CS:CODE,	DS:DATA,	SS:STACK
START:  
	PUSHA
	
	MOV	AX,	DATA
	MOV	DS,	AX
	MOV	SI,	02000H
	MOV	AL,	[SI]
	INC	SI
	CLC
	MOV	CX,     9
K1:	
	MOV	AH,     [SI]
	CMP	AH,	AL
	JL	ELS
	MOV	AL,	AH
	INC	SI
ELS:	LOOP	K1             ; external loop.
	LEA	SI,	ARRAY
	MOV	[SI],	AL
	
	POPA
	MOV	AH,	4CH
	INT	21H
CODE	ENDS

END	START
